{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Xml1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {A:Report_Data} as ({A:Report_Entry} as ({A:CC} as ({A:Descriptor} as string, {A:ID} as ({A:_value_} as string, {A:type} as string)[])[])[])",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     validationMode: 'none',",
				"     namespaces: true,",
				"     namespacePrefixes: ['urn:com.workday.report/CR_Data_Lake_CCH'->'A']) ~> source1",
				"source1 foldDown(unroll({A:Report_Data}.{A:Report_Entry}.{A:CC}.{A:ID}),",
				"     mapColumn(",
				"          {A:Descriptor} = {A:Report_Data}.{A:Report_Entry}.{A:CC}.{A:Descriptor},",
				"          {A:_value_} = {A:Report_Data}.{A:Report_Entry}.{A:CC}.{A:ID}.{A:_value_},",
				"          {A:type} = {A:Report_Data}.{A:Report_Entry}.{A:CC}.{A:ID}.{A:type}",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}