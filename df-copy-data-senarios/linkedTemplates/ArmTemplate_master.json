{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-copy-data-senarios"
		},
		"ADLS2_generaladls2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS2_generaladls2'"
		},
		"AzurePgSql_test_rogers_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzurePgSql_test_rogers'"
		},
		"AzureSql_sample_sql_instance_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSql_sample_sql_instance'"
		},
		"AzureSynapseSynapsewsp01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseSynapsewsp01'"
		},
		"RestFHIR_clientSecret": {
			"type": "secureString",
			"metadata": "Secure string for 'clientSecret' of 'RestFHIR'"
		},
		"SqlServerOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerOnPrem'"
		},
		"ADLS2_generaladls2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://generaladls2.dfs.core.windows.net/"
		},
		"RestFHIR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://telusfhirservice-telushealth.fhir.azurehealthcareapis.com"
		},
		"RestFHIR_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "9e35e2d1-7196-47d4-9f6a-67991b85940e"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-copy-data-senarios_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzurePgSql_test_rogers_connectionString": {
						"value": "[parameters('AzurePgSql_test_rogers_connectionString')]"
					},
					"AzureSql_sample_sql_instance_connectionString": {
						"value": "[parameters('AzureSql_sample_sql_instance_connectionString')]"
					},
					"AzureSynapseSynapsewsp01_connectionString": {
						"value": "[parameters('AzureSynapseSynapsewsp01_connectionString')]"
					},
					"RestFHIR_clientSecret": {
						"value": "[parameters('RestFHIR_clientSecret')]"
					},
					"ADLS2_generaladls2_accountKey": {
						"value": "[parameters('ADLS2_generaladls2_accountKey')]"
					},
					"SqlServerOnPrem_connectionString": {
						"value": "[parameters('SqlServerOnPrem_connectionString')]"
					},
					"RestFHIR_properties_typeProperties_url": {
						"value": "[parameters('RestFHIR_properties_typeProperties_url')]"
					},
					"RestFHIR_properties_typeProperties_clientId": {
						"value": "[parameters('RestFHIR_properties_typeProperties_clientId')]"
					},
					"ADLS2_generaladls2_properties_typeProperties_url": {
						"value": "[parameters('ADLS2_generaladls2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-copy-data-senarios_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-copy-data-senarios_ArmTemplate_0')]"
			]
		},
		{
			"name": "df-copy-data-senarios_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-copy-data-senarios_ArmTemplate_1')]"
			]
		}
	]
}